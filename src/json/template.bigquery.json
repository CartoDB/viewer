{
  "description": "CartoBQTilerLayer declarative example",
  "initialViewState": {
    "latitude": 0,
    "longitude": 0,
    "zoom": 2,
    "pitch": 0,
    "bearing": 0
  },
  "views": [
    {
      "@@type": "MapView",
      "controller": true,
      "mapStyle": "@@#CARTO_BASEMAP.POSITRON"
    }
  ],
  "layers": [
    {
      "@@type": "CartoBQTilerLayer",
      "data": "TYPE A TILESET NAME",
      "credentials": {
        "username": "public",
        "apiKey": "default_public"
      },
      "getFillColor": [232, 133, 113],
      "pointRadiusMinPixels": 2,
      "stroked": false,
      "pickable": true
    }
  ]
}
